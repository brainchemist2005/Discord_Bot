import discord 
from discord.ext import commands

intents = discord.Intents.default()

client = commands.Bot(command_prefix='!', intents=intents)
@client.event
async def on_ready():
    print("The bot is now ready for use!")
    print("-----------------------------")

@client.command()
async def actualite(ctx):
    await ctx.send("Hello, I am a bot")

client.add_command(actualite)

#ca cest la cle du bot que jai mis sur le serveur pour test 
client.run('MTE3MDQ4ODA2MDAwNjM4MzYyNg.GMOzAb.nKEmBQO52ThAPom6bUy7t79nI7zg1KS2gj9nZE')
